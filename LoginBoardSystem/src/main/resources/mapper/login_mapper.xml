<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="edu.doubler.login.dao.userDAO">
	<select 
	id="checkUser"
	parameterType="edu.doubler.login.domain.AppleUser"
	resultType="java.lang.Integer">
		<![CDATA[
		
		SELECT count(*) FROM 
			USER_TB_APPLE 
		WHERE USER_ID = #{id} AND USER_PW = #{pw}
			
		]]>
	</select>
</mapper>